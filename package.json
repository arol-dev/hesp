{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "docker-compose up -d && turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test:unit": "yarn build && (cd apps/hesp && yarn dev &) && (cd apps/hesp && prisma migrate dev) && sleep 10 && (cd apps/hesp && jest)"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.22.0"
  },
  "packageManager": "yarn@1.22.19"
}
